
@echo off
setlocal enabledelayedexpansion
set arrayline[0]=-- Menu --
set arrayline[1]= S(1)-git add
set arrayline[2]= S(2)-commit , push
set arrayline[3]= S(3)-更新最新的程式(fetch , pull , merge)
set arrayline[4]= S(8)-GitHistory
set arrayline[5]= S(9)-cls
set mima=1

:S0 
for /l %%n in (0,1,5) do (
echo !arrayline[%%n]!
)
set /p mima=請決定執行步驟(Def=%mima%)?
if "%mima%"=="1" goto S1
if "%mima%"=="2" goto S2
if "%mima%"=="3" goto S3
if "%mima%"=="8" goto S8
if "%mima%"=="9" goto S9


:S1
echo !arrayline[1]!
git add .
goto S0

:S2
echo !arrayline[2]!
git commit -m "add"
git push --progress "origin"  
TortoiseGitProc.exe /command:log
set mima=1
goto S0

:S3
echo !arrayline[3]!
set mima=1
goto S0

:S8
TortoiseGitProc.exe /command:log
set mima=1
goto S0

:S9
cls
set mima=1
goto S0
