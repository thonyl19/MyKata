<%=Src.triggerEvent.name %>(keyword, fn_cb = null) {
    var _self = this;
    var url = `@<%-Src.triggerEvent.url %>`;
    var param = {keyword};
    var _ajax = {
        url,
        param,
        success(data) {
            if (data == null) return;
            let { Data = [] } = data;
            if (Data == null || Data.length == 0) {
                _self.$Alert.msg_NoData();
            } else {
                _self.ddl_EdcGrp.src = Data;
                if (fn_cb != null) {
                    fn_cb(Data);
                }
            }
        }
    };
    $.submitForm(_ajax);
