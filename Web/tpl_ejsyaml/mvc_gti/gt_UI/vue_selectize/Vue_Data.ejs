<%
    var arr = [];
    Object.keys(Src.attr).map(key=>{
        switch (key) {
            case "selectize_ops":
                var _arr = `ops:${JSON.stringify(Src.attr.selectize_ops,null,4)},`.split('\n');
                arr = $._.concat(arr, _arr);
                //arr.push('test');
                break;
        }
    })
    //0)basic 1)computed 2)syncFiled
    if (Src.mode.v_model!=2) arr.push(`val: "",`);
_%>
<%=Src.name%>: {
    src: [],
    <%- arr.join('\n\t') %>
    old: "",
},