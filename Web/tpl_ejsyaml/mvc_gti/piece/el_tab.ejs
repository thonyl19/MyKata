<%
    arg.ut.fn_inc = include;
_%>
<!-- #region [gt_form_col] -->
<el-tabs v-model="tabIdx">
	<%_ for(var key in arg.Tabs){ 
		var tab = arg.Tabs[key]; _%>	
	<el-tab-pane <%=tab.label%> name="<%=key%>">
		<%_ await arg.ut.echo_ejs(tab,(el)=>{ _%>
		<%-  el _%><%_ }) _%>
	</el-tab-pane>
	<%_} _%>
</el-tabs>
<!-- #endregion [gt_form_col] -->