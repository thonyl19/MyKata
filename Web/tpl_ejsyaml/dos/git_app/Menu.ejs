<%_ 
var idx=1;
var _menu=[],_act=[]
for(var key in Menu){
	_menu.push(`set arrayline[${idx}]= S(${key}) ${Menu[key]}`);
	_act.push(`if "%ans%"=="${key}" goto S${key}`);
	idx++;
}
%> 
@echo off
setlocal enabledelayedexpansion
set arrayline[0]=-- Menu [<%=arg.Title%>] -- 
<%_ _menu.map(e=>{_%>
<%=e%>
<%_})_%>
set mima=1

:S0 
for /l %%n in (0,1,<%=idx-1%>) do (
echo !arrayline[%%n]!
)
set ans=""
set /p ans=請決定執行步驟(Def=%mima%)?
if "%ans%"=="" (ans=mima)
if "%ans%"=="0" goto end
echo "%ans%"
<%_ _act.map(e=>{_%>
<%-e%>
<%_})_%>