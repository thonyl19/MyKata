<el-table empty-text=" " style="width: 100%"
    @@sort-change="sort_change"
    :data="grid.data"
    :default-sort="{prop: 'CREATE_DATE', order: 'ascending'}">
    <%_ _fn(Table_操作欄位,(el)=>{ %>
    <%- el %><% }) %>
    <el-table-column    min-width="115" 
                        prop="ENCODE_FORMAT_NO" 
                        sortable="custom"
                        :sort-orders="$UT.sort_order"
                        label="@RES.BLL.Face.lblSetSequenceNo">
        <template slot-scope="scope">
            <!--需要調整-->
            <el-button type="text" @@click="act_Item(scope.row.ENCODE_FORMAT_SID)">{{scope.row.ENCODE_FORMAT_NO}}</el-button>
        </template>
    </el-table-column>
    <%_ _fn(TableColumn,(el)=>{ %>
    <%- el %><% }) %>
</el-table>
<el-pagination background
                @@size-change="e_SizeChange"
                @@current-change="e_CurrentChange"
                :current-page="grid.Page.Index"
                :page-sizes="[5,10,15]"
                :page-size="grid.Page.Size"
                layout="prev, pager, next"
                :total="grid.row_count"></el-pagination>
