      </HelpUrl>
      <Shortcut>
      </Shortcut>
    </Header>
    <Snippet>
      <Code Language="javascript" Delimiter="$"><![CDATA[__gt_test(key, val)
	{
		var _self = this;
		let[row0] = this.grid.data;
		switch (key)
		{
			case "訊息":
				if (row0 == null) return "當前 grid 沒有資料!";
				this.form.MESSAGE = val || row0.MESSAGE.substr(0, 3);
				this.click_Query();
				break;
			case "部門代碼":
				if (row0 == null) return "當前 grid 沒有資料!";
				this.form.DEPT_SID = val || row0.DEPT_SID;
				this.click_Query();
				break;
	},]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>